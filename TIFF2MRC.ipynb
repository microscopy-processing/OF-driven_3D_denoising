   "source": [
    "# TIFF to MRC converter"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {
    "id": "IIUxh4uStrQJ"
   },
   "outputs": [],
   "source": [
    "import mrcfile\n",
    "import skimage.io\n",
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "stack = skimage.io.imread(\"stack.tif\", plugin=\"tifffile\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    " with mrcfile.new(\"stack.mrc\", overwrite=True) as mrc:\n",
    "    mrc.set_data(stack.astype(np.float32))\n",
    "    mrc.data"
   ]
  },
  {
